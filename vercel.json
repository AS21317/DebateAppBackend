{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "headers": [
        {
          "source": "**/*.js",
          "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
        }
      ]
    }
  ]
}